  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "colors": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.3.4",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
